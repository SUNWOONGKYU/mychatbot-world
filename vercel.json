{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "."
            }
        }
    ],
    "headers": [
        {
            "source": "/js/(.*)",
            "headers": [
                { "key": "Content-Type", "value": "application/javascript; charset=utf-8" }
            ]
        },
        {
            "source": "/css/(.*)",
            "headers": [
                { "key": "Content-Type", "value": "text/css; charset=utf-8" }
            ]
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}